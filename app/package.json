{
  "name": "node-express-es6-starter",
  "version": "0.4.2",
  "description": "A sample Node.js app using Express 4",
  "scripts": {
    "start:dev": "nodemon --watch views --watch src --watch public -e js,twig,html,css app.js",
    "start:dep": "ENV=deployment node app.js",
    "start": "npm run start:dev",
    "todo": "leasot ./src/**/*.js app.js --tags error,info || true"
  },
  "dependencies": {
    "express": "4.14.0",
    "gulp-messenger": "0.25.3",
    "lodash": "4.17.2",
    "twig": "0.10.1"
  },
  "babel": {
    "presets": [
      "es2015",
      "stage-0"
    ],
    "ignore": [
      "node_modules"
    ]
  },
  "devDependencies": {
    "babel-core": "6.18.2",
    "babel-preset-es2015": "6.18.0",
    "leasot": "4.2.0",
    "nodemon": "1.11.0",
    "todo-webpack-plugin": "1.5.1"
  },
  "reveal": true
}
